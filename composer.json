    "keywords": [
        "feature switch",
        "feature toggle"
    ],
    "homepage": "http://github.com/mpociot/feature-switch",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
